test legalizer
set enable_simd
target x86_64 skylake

function %icmp_ne_32x4(i32x4, i32x4) -> b32x4 {
ebb0(v0: i32x4, v1: i32x4):
    v2 = icmp ne v0, v1
    ; check: v3 = icmp eq v0, v1
    ; nextln: v4 = vconst.b32x4 0xffffffffffffffffffffffffffffffff
    ; nextln: v2 = bxor v4, v3
    return v2
}
